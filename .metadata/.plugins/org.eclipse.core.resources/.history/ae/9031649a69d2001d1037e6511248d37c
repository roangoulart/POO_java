package modelo;

import java.util.ArrayList;

public class Conta {

	private String titularDaConta;
	private int tipo;
	private double saldo = 0;
	protected ArrayList<Movimentacao> listaMovimentacoes;

	public static double depositar(double saldo, double depositar) {
		saldo = depositar + saldo;
		return saldo;
	}

	public static double sacar(double saldo, double saque) {
		saldo = saque - saldo;
		return saldo;
	}

	public String getTituarDaConta() {
		return titularDaConta;
	}

	public void setTitularDaConta(String titularDaConta) {
		this.titularDaConta = titularDaConta;
	}

	public void gerarExtrato() {
	}

	private void gerarExtratoDepositos() {

	}

	private void gerarExtratoSaques() {

	}

}
